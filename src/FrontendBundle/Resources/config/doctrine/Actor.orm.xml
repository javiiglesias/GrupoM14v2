<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FrontendBundle\Entity\Actor" table="actor">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nif" type="string" column="NIF" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nom" type="string" column="Nom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cognom1" type="string" column="Cognom1" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cognom2" type="string" column="Cognom2" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sexe" type="string" column="Sexe" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="foto" type="string" column="Foto" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idObra" type="integer" column="id_obra" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
