<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FrontendBundle\Entity\Obra" table="obra">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="Nom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcio" type="string" column="Descripcio" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datainici" type="date" column="DataInici" nullable="false"/>
    <field name="datafi" type="date" column="DataFi" nullable="false"/>
    <field name="idAgencia" type="integer" column="id_agencia" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
